# Google service account credentials used to provision cluster resources
export GOOGLE_APPLICATION_CREDENTIALS=/path/to/credentials.json

# Indicate that inventory should give NAT ip for instances rather than internal
# IP addresses
export GCP_ANSIBLE_INVENTORY_USE_NAT_IP=true

# Cluster to provision
export DEMO_CLUSTER_NAME=gcp-demo-sbx-okd3

